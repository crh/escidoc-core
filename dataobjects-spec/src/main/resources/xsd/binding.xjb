    <bindings node="//xs:element[@name='explainRequest']">
      <class name="ExplainRequestTO" />
    </bindings>
    <bindings node="//xs:element[@name='explainResponse']">
      <class name="ExplainResponseTO" />
    </bindings>
    <!-- java types -->
    <bindings node="//xs:element[@name='startRecord']">
