    </bindings>
    <bindings node="//xs:element[@name='responsePosition']">
      <javaType name="org.apache.axis.types.NonNegativeInteger"/>
    </bindings>
    <bindings node="//xs:element[@name='maximumTerms']">
      <javaType name="org.apache.axis.types.PositiveInteger"/>
    </bindings>
  </bindings>
